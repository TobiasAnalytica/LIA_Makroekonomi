table '_Measures (Diagnostics)'
	lineageTag: cb6934d5-fe74-4004-8e38-c932c2c4761c

	measure 'Latest Run Date (Per Job)' =
			
			VAR job = SELECTEDVALUE ( Run[JobName] )
			RETURN CALCULATE ( MAX ( Run[StartedAt] ), FILTER ( ALL ( Run ), Run[JobName] = job ) )
		lineageTag: faa0b4bb-9946-4848-acaa-c265045acd41

	measure 'Latest RunId (Per Job)' =
			
			VAR job   = SELECTEDVALUE ( Run[JobName] )
			VAR lastD = [Latest Run Date (Per Job)]
			RETURN
			MAXX ( FILTER ( ALL ( Run ), Run[JobName] = job && Run[StartedAt] = lastD ), Run[RunId] )
		lineageTag: 4129346b-815a-41e3-82ad-27d6cc8f6533

	measure 'Latest Status (Per Job)' =
			
			VAR rid = [Latest RunId (Per Job)]
			RETURN MAXX ( FILTER ( ALL ( Run ), Run[RunId] = rid ), Run[Status] )
		lineageTag: 68f704ea-62cd-444c-81a2-d5c107dca8f3

	measure 'Status – Light (Per Job)' =
			
			IF ( [Latest Status (Per Job)] = "S", 1, 0 )   -- 1 = grön, 0 = röd
		formatString: 0
		lineageTag: 8baaf574-b165-4702-8da4-8906deb0cf27

	measure 'Status – Color (Per Job)' =
			
			IF ( [Status – Light (Per Job)] = 1, "#22c55e", "#ef4444" )
		lineageTag: 42496577-8e87-4d7b-9010-869ddf3ed352

	measure 'Latest Message (Per Job)' =
			
			VAR rid = [Latest RunId (Per Job)]
			RETURN MAXX ( FILTER ( ALL ( Run ), Run[RunId] = rid ), Run[Message] )
		lineageTag: 172f5afb-8b2b-4d1f-9371-ca81d2d2c965

	measure 'Runs – Duration (min)' =
			
			VAR s = SELECTEDVALUE ( Run[StartedAt] )
			VAR e = SELECTEDVALUE ( Run[EndedAt] )
			RETURN IF ( NOT ISBLANK ( s ) && NOT ISBLANK ( e ), DATEDIFF ( s, e, MINUTE ) )
		formatString: 0
		lineageTag: ed0832c9-3104-4744-8a91-4cb3dd0d00e2

	measure 'Errors – Count (Selected)' = COUNTROWS ( 'Error' )
		formatString: 0
