      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{A017A1D6-82C4-449D-8C3E-61DF27798648}"
          SQLTask:SqlStatementSource="-- Steg 1: Definiera start- och slutdatum&#xA;DECLARE @StartDate DATE = '1960-01-01';&#xA;DECLARE @EndDate   DATE = '2030-12-31';&#xA;&#xA;-- Steg 2: Använd en rekursiv CTE för att generera alla datum i intervallet&#xA;WITH DateCTE AS (&#xA;    SELECT &#xA;        @StartDate AS DateValue&#xA;    UNION ALL&#xA;    SELECT&#xA;        DATEADD(DAY, 1, DateValue)&#xA;    FROM&#xA;        DateCTE&#xA;    WHERE&#xA;        DateValue &lt; @EndDate&#xA;)&#xA;-- Steg 3: Infoga i DimDate genom att extrahera alla kolumner&#xA;INSERT INTO DimDate (&#xA;    DateKey,&#xA;    DateValue,&#xA;    YearNumber,&#xA;    MonthNumber,&#xA;    DayNumber,&#xA;    DayOfWeek,&#xA;    WeekNumber,&#xA;    QuarterNumber,&#xA;    MonthName,&#xA;    DayName&#xA;)&#xA;SELECT&#xA;    CAST(CONVERT(CHAR(8), DateValue, 112) AS INT) AS DateKey,  -- 112 ger 'YYYYMMDD' som string&#xA;    DateValue,&#xA;    YEAR(DateValue) AS YearNumber,&#xA;    MONTH(DateValue) AS MonthNumber,&#xA;    DAY(DateValue) AS DayNumber,&#xA;    DATEPART(WEEKDAY, DateValue) AS DayOfWeek,                -- 1=Sunday … 7=Saturday i SQL Server; justera om du vill ha ISO (1=Monday)&#xA;    DATEPART(ISO_WEEK, DateValue) AS WeekNumber,              -- ISO-veckonummer (i SQL Server 2012+)&#xA;    DATEPART(QUARTER, DateValue) AS QuarterNumber,&#xA;    DATENAME(MONTH, DateValue) AS MonthName,                  -- Månadens namn på serverspråket&#xA;    DATENAME(WEEKDAY, DateValue) AS DayName                   -- Veckodagens namn&#xA;FROM&#xA;    DateCTE&#xA;OPTION (MAXRECURSION 0);  -- Tillåter fler än standard 100 rekursioner&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
