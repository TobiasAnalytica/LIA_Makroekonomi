                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH AllRegion AS (
    SELECT CAST(geo AS NVARCHAR(10)) AS Region, CAST([Geopolitical entity (reporting)] AS NVARCHAR(120)) AS Description FROM gdp_cleaned
    UNION ALL
    SELECT CAST(geo AS NVARCHAR(10)) AS Region, CAST([Geopolitical entity (reporting)] AS NVARCHAR(120)) AS Description FROM employment_cleaned
    UNION ALL
    SELECT CAST(geo AS NVARCHAR(10)) AS Region, CAST([Geopolitical entity (reporting)] AS NVARCHAR(120)) AS Description FROM unemployment_cleaned
    UNION ALL
    SELECT CAST(geo AS NVARCHAR(10)) AS Region, CAST([Geopolitical entity] AS NVARCHAR(120)) AS Description FROM population_cleaned
    UNION ALL
    SELECT CAST(geo AS NVARCHAR(10)) AS Region, CAST([Geopolitical entity (reporting)] AS NVARCHAR(120)) AS Description FROM job_vacancies_cleaned
    UNION ALL
    SELECT CAST(geo AS NVARCHAR(10)) AS Region, CAST([Geopolitical entity (reporting)] AS NVARCHAR(120)) AS Description FROM money_market_interest_rates_cleaned
    UNION ALL
    SELECT CAST(geo AS NVARCHAR(10)) AS Region, CAST([Geopolitical entity (reporting)] AS NVARCHAR(120)) AS Description FROM hicp_index_cleaned
    UNION ALL
    SELECT CAST(geo AS NVARCHAR(10)) AS Region, CAST([Geopolitical entity (reporting)] AS NVARCHAR(120)) AS Description FROM government_debt_cleaned
)
SELECT DISTINCT
       Region,
       Description
  FROM AllRegion
ORDER BY Description;</property>
