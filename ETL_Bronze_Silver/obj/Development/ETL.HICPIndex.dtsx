using System;  // Grundläggande systemfunktioner
using System.Net;  // Hantering av nätverksprotokoll och säkerhetsinställningar
using System.Net.Http;  // HTTP-klient för att skicka förfrågningar
using System.Data.SqlClient;  // SQL Server-anslutning och kommandon
using System.Globalization;  // Hantering av kultur- och formatinställningar
using Microsoft.VisualBasic.FileIO;  // TextFieldParser för CSV-parsning (paket: Microsoft.VisualBasic)
using Microsoft.SqlServer.Dts.Runtime;  // SSIS Runtime-objekt (Dts, Events, etc.)
using System.IO;  // Fil- och strömhantering
using System.Collections.Generic;  // Generiska samlingar (ej använt men kvar för framtida behov)
using System.Text;  // Textkodning och stränghantering
#endregion

namespace ST_a0807b8b059845579c8797e5a472b130
{
    // Anger inmatningspunkten för SSIS Script Task
    [Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {
        /// <summary>
        /// Huvudmetod som körs av SSIS-paketet.
        /// Fångar eventuella fel och sätter TaskResult.
        /// </summary>
        public void Main()
        {
            try
            {
                // Hämta och skriv in HICP-indexdata från Eurostats API
                FetchAndInsertHICPData();

                // Om allt gick bra, sätt resultatet till Success
                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                // Vid fel: logga ett felmeddelande och markera Task som failure
                Dts.Events.FireError(
                    0,                  // error ID
                    "Script Task",    // kategori
                    ex.ToString(),      // felbeskrivning
                    string.Empty,       // nodnamn
                    0                   // radnummer
                );
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

        /// <summary>
        /// Hämtar HICP-indexdata som CSV från Eurostats API och skriver in i ett SQL Server-databasbord.
        /// </summary>
        private void FetchAndInsertHICPData()
        {
            // Tvinga TLS 1.2 för säker kommunikation
            ServicePointManager.SecurityProtocol = SecurityProtocolType.Tls12;

            // Sätt upp URL för Eurostats API-fråga med önskade parametrar
            var apiUrl = "https://ec.europa.eu/eurostat/api/dissemination/sdmx/3.0/data/dataflow/ESTAT/prc_hicp_midx/1.0/*.*.*.*?" +
                         "c[freq]=M&" +
                         "c[unit]=I96,I05,I15&" +
                         "c[coicop]=CP00,CP01,CP02,CP03,CP04,CP05,CP06,CP07,CP08,CP09,CP10,CP11,CP12&" +
                         "c[geo]=EU,EU27_2020,EU28,EA,EA20,EA19,BE,BG,CZ,DK,DE,EE,IE,EL,ES,FR,HR,IT,CY,LV,LT,LU,HU,MT,NL,AT,PL,PT,RO,SI,SK,FI,SE,EEA,IS,NO,CH,UK,ME,MK,AL,RS,TR,XK,US&" +
                         "compress=false&format=csvdata&formatVersion=2.0&lang=en&labels=name";

            // Logga URL för felsökning utan att avbryta exekveringen
            bool fireAgain = false;
            Dts.Events.FireInformation(
                0,          // information ID
                "Debug",   // kategori
                apiUrl,     // meddelande
                String.Empty,
                0,
                ref fireAgain
            );

            // Skapa HttpClient för att hämta data
            using (var client = new HttpClient())
            {
                // ⬇️ Timeout inställning – öka till 10 minuter
                client.Timeout = TimeSpan.FromMinutes(10);

                // Hämta CSV-data som sträng
                var csvData = client.GetStringAsync(apiUrl).Result;

                // Dela upp data i rader baserat på radbrytningar
                var lines = csvData.Split(new[] { "\r\n", "\n" }, StringSplitOptions.None);

                // Om inga rader eller bara header, avbryt
                if (lines.Length < 2)
                    return;

                // Anslut till SQL Server-databasen DW_Bronze
                using (var conn = new SqlConnection("Data Source=.;Initial Catalog=LIA_DB_Bronze;Integrated Security=SSPI;"))
                {
                    conn.Open();

                    // Loopa igenom varje rad, hoppa över header (i = 1)
                    for (int i = 1; i < lines.Length; i++)
                    {
                        var line = lines[i];
                        if (string.IsNullOrWhiteSpace(line))
                            continue;

                        using (var parser = new TextFieldParser(new StringReader(line)))
                        {
                            parser.SetDelimiters(",");
                            parser.HasFieldsEnclosedInQuotes = true;
                            string[] cols = parser.ReadFields();

                            // Säkerställ att vi har tillräckligt med kolumner
                            if (cols.Length < 19)
                                continue;

                            // Mappa varje CSV-kolumn till en variabel
                            var structure = cols[0];
                            var structureId = cols[1];
                            var structureName = cols[2];
                            var freq = cols[3];
                            var timeFreq = cols[4];
                            var unit = cols[5];
                            var unitOfMeasure = cols[6];
                            var coicop = cols[7];
                            var classification = cols[8];
                            var geo = cols[9];
                            var geopoliticalEntity = cols[10];
                            var timePeriod = cols[11];
                            var time = cols[12];
                            var obsValueStr = cols[13];
                            var observationValue = cols[14];
                            var obsFlag = cols[15];
                            var observationStatusFlag = cols[16];
                            var confStatus = cols[17];
                            var confidentialityStatus = cols[18];

                            object obsValueParam = DBNull.Value;
                            if (!string.IsNullOrEmpty(obsValueStr) &&
                                decimal.TryParse(obsValueStr, NumberStyles.Any, CultureInfo.InvariantCulture, out decimal obsVal))
                            {
                                obsValueParam = obsVal;
                            }

                            // Infoga dataposten i tabellen hicp_index_raw
                            using (var cmd = new SqlCommand(
                                "INSERT INTO hicp_index_raw ([STRUCTURE], [STRUCTURE_ID], [STRUCTURE_NAME], [freq], [Time frequency], [unit], [Unit of measure], [coicop], [Classification of individual consumption by purpose (COICOP)], [geo], [Geopolitical entity (reporting)], [TIME_PERIOD], [Time], [OBS_VALUE], [Observation value], [OBS_FLAG], [Observation status (Flag) V2 structure], [CONF_STATUS], [Confidentiality status (flag)]) " +
                                "VALUES (@structure, @structureId, @structureName, @freq, @timeFreq, @unit, @unitOfMeasure, @coicop, @classification, @geo, @geopoliticalEntity, @timePeriod, @time, @obsValueParam, @observationValue, @obsFlag, @observationStatusFlag, @confStatus, @confidentialityStatus)", conn))
                            {
                                cmd.Parameters.AddWithValue("@structure", structure);
                                cmd.Parameters.AddWithValue("@structureId", structureId);
                                cmd.Parameters.AddWithValue("@structureName", structureName);
                                cmd.Parameters.AddWithValue("@freq", freq);
                                cmd.Parameters.AddWithValue("@timeFreq", timeFreq);
                                cmd.Parameters.AddWithValue("@unit", unit);
                                cmd.Parameters.AddWithValue("@unitOfMeasure", unitOfMeasure);
                                cmd.Parameters.AddWithValue("@coicop", coicop);
                                cmd.Parameters.AddWithValue("@classification", classification);
                                cmd.Parameters.AddWithValue("@geo", geo);
                                cmd.Parameters.AddWithValue("@geopoliticalEntity", geopoliticalEntity);
                                cmd.Parameters.AddWithValue("@timePeriod", timePeriod);
                                cmd.Parameters.AddWithValue("@time", time);
                                cmd.Parameters.AddWithValue("@obsValueParam", obsValueParam);
                                cmd.Parameters.AddWithValue("@observationValue", observationValue);
                                cmd.Parameters.AddWithValue("@obsFlag", obsFlag);
                                cmd.Parameters.AddWithValue("@observationStatusFlag", observationStatusFlag);
                                cmd.Parameters.AddWithValue("@confStatus", confStatus);
                                cmd.Parameters.AddWithValue("@confidentialityStatus", confidentialityStatus);

                                cmd.ExecuteNonQuery();
                            }
                        }
                    }
                }
            }
        }

        /// <summary>
        /// Enum för att underlätta hantering av SSIS result-koder.
        /// Success = 0, Failure = 1
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        }
    }
}]]></ProjectItem>
