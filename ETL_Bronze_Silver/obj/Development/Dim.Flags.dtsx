                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH AllFlags AS (
    SELECT 
        CAST(OBS_FLAG AS NVARCHAR(10)) AS OBS_FLAG,
        CAST([Observation status (Flag) V2 structure] AS NVARCHAR(70)) AS Description
      FROM gdp_cleaned

    UNION ALL
    SELECT 
        CAST(OBS_FLAG AS NVARCHAR(10)) AS OBS_FLAG,
        CAST([Observation status (Flag) V2 structure] AS NVARCHAR(70)) AS Description
      FROM employment_cleaned

    UNION ALL
    SELECT 
        CAST(OBS_FLAG AS NVARCHAR(10)) AS OBS_FLAG,
        CAST([Observation status (Flag) V2 structure] AS NVARCHAR(70)) AS Description
      FROM unemployment_cleaned

    UNION ALL
    SELECT 
        CAST(OBS_FLAG AS NVARCHAR(10)) AS OBS_FLAG,
        CAST([Observation status] AS NVARCHAR(70)) AS Description
      FROM population_cleaned

    UNION ALL
    SELECT 
        CAST(OBS_FLAG AS NVARCHAR(10)) AS OBS_FLAG,
        CAST([Observation status (Flag) V2 structure] AS NVARCHAR(70)) AS Description
      FROM job_vacancies_cleaned

    UNION ALL
    SELECT 
        CAST(OBS_FLAG AS NVARCHAR(10)) AS OBS_FLAG,
        CAST([Observation status (Flag) V2 structure] AS NVARCHAR(70)) AS Description
      FROM money_market_interest_rates_cleaned

    UNION ALL
    SELECT 
        CAST(OBS_FLAG AS NVARCHAR(10)) AS OBS_FLAG,
        CAST([Observation status (Flag) V2 structure] AS NVARCHAR(70)) AS Description
      FROM hicp_index_cleaned

    UNION ALL
    SELECT 
        CAST(OBS_FLAG AS NVARCHAR(10)) AS OBS_FLAG,
        CAST([Observation status (Flag) V2 structure] AS NVARCHAR(70)) AS Description
      FROM government_debt_cleaned
)
SELECT DISTINCT
       OBS_FLAG,
       Description
FROM AllFlags
ORDER BY OBS_FLAG;</property>
